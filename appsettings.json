{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "VAR_CONNECT_STRING",
    "StorageConnection": "VAR_STORAGE_CONNECT"
  },
  "Authentication": {
    "SecurityKey": "VAR_SECURITY_KEYS",
    "ValidIssuer": "https://VAR_VALID_PRODUCER"
  },
  "Storage": {
    "Container": "VAR_STORAGE_CONTAINER"
  },
  "DefaultPassword": "VAR_DEFAULTS_PASSWORD",
  "CoreAPI": {
    "Url": "VAR_CORES_API_URL",
    "Key": "VAR_CORES_API_KEY"
  }
}
